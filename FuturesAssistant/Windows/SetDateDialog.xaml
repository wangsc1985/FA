<my:DialogBase xmlns:my="clr-namespace:FuturesAssistant.Windows"   x:Class="FuturesAssistant.Windows.SetDateDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="选择删除起始日期" x:Name="_dialog"  Height="180" Width="300">
    <Grid>
        <GroupBox HorizontalAlignment="Center" VerticalAlignment="Center">
            <StackPanel Margin="10" Orientation="Vertical">
                <DatePicker Name="_dateTimePicker" Margin="10,10,10,15"></DatePicker>
                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                    <Button IsDefault="True" Name="_button确定" Margin="10" Width="80" Content="确定" Click="_button确定_Click"></Button>
                    <Button Name="_button取消" Margin="10" Width="80" Content="取消" Click="_button取消_Click"></Button>
                </StackPanel>
            </StackPanel>
        </GroupBox>
    </Grid>
</my:DialogBase>
