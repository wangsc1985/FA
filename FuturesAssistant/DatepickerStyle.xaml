<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<ControlTemplate x:Key="DatePickerControlTemplate1" TargetType="{x:Type DatePicker}">
		<Grid>
			<TextBox x:Name="textBox" Text="{TemplateBinding Text}" HorizontalAlignment="Stretch" Margin="0" VerticalAlignment="Stretch" BorderThickness="3,0" IsEnabled="False"/>
		</Grid>
	</ControlTemplate>
	<!-- 应该在此定义资源字典条目。-->
</ResourceDictionary>